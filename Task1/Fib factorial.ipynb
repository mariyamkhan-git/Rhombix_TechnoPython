   "source": [
    "def fibfact(number):\n",
    "    if number <= 0:\n",
    "        return 0\n",
    "    elif number == 1:\n",
    "        return 1\n",
    "    else: \n",
    "        return fibfact(number-1)+fibfact(number-2)"
   ]
